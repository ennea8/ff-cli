{
  "name": "ff-cli",
  "version": "0.1.0",
  "description": "Solana token batch transfer CLI tool",
  "main": "dist/cli.js",
  "bin": {
    "ff": "./bin/ff"
  },
  "scripts": {
    "test": "echo \"No tests specified\" && exit 0",
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "pnpx tsx cli/src/cli.ts",
    "prepare": "pnpm run build",
    "prepublishOnly": "pnpm test",
    "version": "git add -A",
    "postversion": "git push && git push --tags",
    "install-global": "pnpm link && pnpm link --global"
  },
  "keywords": [
    "solana",
    "cli",
    "blockchain",
    "token",
    "transfer",
    "batch",
    "crypto"
  ],
  "author": "",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "https://github.com/ennea8/ff-cli.git"
  },
  "dependencies": {
    "@solana/spl-token": "^0.4.13",
    "@solana/web3.js": "^1.98.4",
    "bs58": "^6.0.0",
    "chalk": "^5.5.0",
    "commander": "^14.0.0",
    "csv-parse": "^6.1.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^17.2.1",
    "pino": "^9.8.0",
    "pino-pretty": "^13.1.1",
    "tsx": "^4.20.3"
  },
  "devDependencies": {
    "tsx": "^4.20.3",
    "typescript": "^5.9.2"
  }
}
